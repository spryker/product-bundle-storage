<?xml version="1.0"?>
<transfers
        xmlns="spryker:transfer-01"
        xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
        xsi:schemaLocation="spryker:transfer-01 http://static.spryker.com/transfer-01.xsd">

    <transfer name="ProductBundleStorage">
        <property name="idProductConcreteBundle" type="int"/>
        <property name="bundledProducts" type="ProductForBundleStorage[]" singular="bundledProduct"/>
    </transfer>

    <transfer name="ProductForBundleStorage">
        <property name="idProductConcrete" type="int"/>
